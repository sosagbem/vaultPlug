<h1>Listing shortened_urls</h1>

<table>
  <tr>
    <th>Website</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shortened_urls.each do |shortened_url| %>
  <tr>
    <td><%= shortened_url.website %></td>
    <td><%= shortened_url.url %></td>
    <td><%= link_to 'Show', shortened_url %></td>
    <td><%= link_to 'Edit', edit_shortened_url_path(shortened_url) %></td>
    <td><%= link_to 'Destroy', shortened_url, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Shortened url', new_shortened_url_path %>
